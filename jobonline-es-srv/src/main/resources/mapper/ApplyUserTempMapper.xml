<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jobtbao.oauth.srv.applyuser.dao.ApplyUserTempMapper">

	<select id="get" resultType="com.jobtbao.oauth.srv.applyuser.bean.ApplyUserTemp">
		select `id`,`phone`,`uid`,`create_date` from apply_user_temp where id = #{value}
	</select>
    <select id="getByPhone" resultType="com.jobtbao.oauth.srv.applyuser.bean.ApplyUserTemp">
		select `id`,`phone`,`uid`,`create_date` from apply_user_temp where phone = #{phone}
	</select>
	<select id="list" resultType="com.jobtbao.oauth.srv.applyuser.bean.ApplyUserTemp">
		select `id`,`phone`,`uid`,`create_date` from apply_user_temp
         <where>  
		  		  <if test="id != null and id != ''"> and id = #{id} </if>
		  		  <if test="phone != null and phone != ''"> and phone = #{phone} </if>
		  		  <if test="uid != null and uid != ''"> and uid = #{uid} </if>
		  		  <if test="createDate != null and createDate != ''"> and create_date = #{createDate} </if>
		 </where>
       limit 1;
	</select>
	
 	<select id="count" resultType="int">
		select count(*) from apply_user_temp
		 <where>  
		  		  <if test="id != null and id != ''"> and id = #{id} </if>
		  		  <if test="phone != null and phone != ''"> and phone = #{phone} </if>
		  		  <if test="uid != null and uid != ''"> and uid = #{uid} </if>
		  		  <if test="createDate != null and createDate != ''"> and create_date = #{createDate} </if>
		  		</where>
	</select>
	 
	<insert id="save" parameterType="com.jobtbao.oauth.srv.applyuser.bean.ApplyUserTemp">
		insert into apply_user_temp
		(
			`id`, 
			`phone`, 
			`uid`, 
			`create_date`
		)
		values
		(
			#{id}, 
			#{phone}, 
			#{uid}, 
			#{createDate}
		)
	</insert>
	 
	<update id="update" parameterType="com.jobtbao.oauth.srv.applyuser.bean.ApplyUserTemp">
		update apply_user_temp 
		<set>
			<if test="uid != null">`uid` = #{uid}, </if>
			<if test="createDate != null">`create_date` = #{createDate},</if>
			<if test="bindDate != null">`bind_date` = #{bindDate},</if>
		</set>
		where id = #{id}
	</update>
	
	<delete id="remove">
		delete from apply_user_temp where id = #{value}
	</delete>
	
	<delete id="batchRemove">
		delete from apply_user_temp where id in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>